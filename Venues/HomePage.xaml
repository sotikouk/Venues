<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Venues"
             x:Class="Venues.HomePage">
    <TabbedPage.ToolbarItems>
        <ToolbarItem x:Name="AddButton" Text="Add"
                     IconImageSource="add.png"
                     Clicked="AddButton_OnClicked"/>
    </TabbedPage.ToolbarItems>
    <local:HistoryPage Title="History"
                       IconImageSource="home.png"/>
    <local:MapPage Title="Map"
                   IconImageSource="map.png"/>
    <local:ProfilePage Title="Profile"
                       IconImageSource="user.png"/>
    
</TabbedPage>